{
	"root": true,
	"env": {
		"browser": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"extends": ["airbnb", "prettier"],
	"plugins": ["react", "jsx-a11y", "import", "react-hooks", "prettier"],
	"rules": {
		"no-console": "warn",
		"no-param-reassign": "off",
		"no-underscore-dangle": "off",
		"no-plusplus": "off"
	}
}
